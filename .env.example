# Banco SEI (Origem - Produção)
SEI_DB_HOST=seu_host_sei.pi.gov.br
SEI_DB_PORT=5432
SEI_DB_NAME=sei
SEI_DB_USER=seu_usuario
SEI_DB_PASSWORD=sua_senha
SEI_DB_SCHEMA=sei_processo

# Banco Local (Destino - Docker Compose)
LOCAL_DB_HOST=localhost
LOCAL_DB_PORT=5432
LOCAL_DB_NAME=sei_ontology
LOCAL_DB_USER=sei_user
LOCAL_DB_PASSWORD=sei_password

# Neo4J
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=sei_neo4j_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# MinIO
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET=sei-documentos

# Extração (Banco SEI direto)
BATCH_SIZE=1000
MAX_WORKERS=4

# API SEI (Consulta via API REST)
SEI_API_BASE_URL=https://api.sei.pi.gov.br
SEI_API_USER=seu_usuario@orgao.pi.gov.br
SEI_API_PASSWORD=sua_senha_api
SEI_API_ORGAO=GOV-PI
SEI_API_ID_UNIDADE=000000  # ID da unidade padrão para consultas
SEI_API_MAX_CONCURRENT=10  # Máximo de requisições simultâneas
SEI_API_MAX_CONCURRENT_DOWNLOADS=5  # Máximo de downloads simultâneos
SEI_API_TIMEOUT=30  # Timeout em segundos

# MinIO configuração adicional
MINIO_SECURE=false  # true para HTTPS
